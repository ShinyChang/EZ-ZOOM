{
  "name": "EZ Zoom",
  "version": "1.1",
  "description": "Press + - * can zoom just like Opera.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
  } ],
  "icons": {
      "128": "icon.png",
      "48": "icon.png"
   },
  "permissions": [
     "http://*/*", "https://*/*"
  ]

}